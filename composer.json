{
  "name": "bizley/podium-client",
  "type": "yii2-extension",
  "description": "Yii 2 Forum Client.",
  "keywords": ["forum", "yii2", "extension", "podium", "module", "bizley"],
  "license": "Apache-2.0",
  "support": {
    "issues": "https://github.com/bizley/yii2-podium-client/issues",
    "wiki": "https://github.com/bizley/yii2-podium-client/wiki",
    "source": "https://github.com/bizley/yii2-podium-client"
  },
  "authors": [
    {
      "name": "Pawel Bizley Brzozowski",
      "email": "pawel@positive.codes"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "bizley/podium-api": "dev-master",
    "php": ">=7.1.0",
    "yiisoft/yii2": ">=2.0.15.1 <2.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.1.0",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "bizley\\podium\\client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "bizley\\podium\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
